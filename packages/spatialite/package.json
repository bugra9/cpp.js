    "homepage": "https://www.gaia-gis.it/fossil/libspatialite/index",
    "license": "SEE LICENSE IN https://website-archive.mozilla.org/www.mozilla.org/mpl/MPL/boilerplate-1.1/mpl-tri-license-html",
