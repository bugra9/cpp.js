{
    "name": "@cpp.js/package-webp",
    "version": "1.0.0",
    "nativeVersion": "1.5.0",
    "description": "This package provides a WebP library compiled with Cpp.js, enabling seamless usage of WebP functionalities in JavaScript, WebAssembly and React Native projects. It supports efficient image processing for web and mobile applications.",
    "homepage": "https://github.com/bugra9/cpp.js/tree/main/packages/cppjs-package-webp#readme",
    "repository": "https://github.com/bugra9/cpp.js.git",
    "license": "BSD-3-Clause",
    "type": "module",
    "scripts": {
        "build": "cppjs build",
        "build:wasm": "cppjs build -p WebAssembly",
        "build:android": "cppjs build -p Android",
        "build:ios": "cppjs build -p iOS",
        "clear": "rm -rf .cppjs dist *.xcframework",
        "prepublishOnly": "cppjs build"
    },
    "devDependencies": {
        "cpp.js": "workspace:^"
    },
    "keywords": [
        "webp",
        "libwebp",
        "cpp.js-package",
        "webassembly",
        "react-native"
    ]
}
